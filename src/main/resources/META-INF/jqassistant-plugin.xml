<jqa-plugin:jqassistant-plugin
        xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.1" name="C AST">
    <description>Provides a scanner for C source code as XML file.</description>
    <model>
        <class>org.jqassistant.contrib.plugin.c.api.model.CAstFileDescriptor</class>
        <class>org.jqassistant.contrib.plugin.c.api.model.CDescriptor</class>
        <class>org.jqassistant.contrib.plugin.c.api.model.TranslationUnitDescriptor</class>
    	<class>org.jqassistant.contrib.plugin.c.api.model.FunctionDescriptor</class>
    	<class>org.jqassistant.contrib.plugin.c.api.model.EnumDescriptor</class>
    	<class>org.jqassistant.contrib.plugin.c.api.model.ParameterDescriptor</class>
    	<class>org.jqassistant.contrib.plugin.c.api.model.StructDescriptor</class>
    	<class>org.jqassistant.contrib.plugin.c.api.model.TypedDescriptor</class>
    	<class>org.jqassistant.contrib.plugin.c.api.model.TypeDescriptor</class>
    	<class>org.jqassistant.contrib.plugin.c.api.model.UnionDescriptor</class>
    	<class>org.jqassistant.contrib.plugin.c.api.model.VariableDescriptor</class>
    	<class>org.jqassistant.contrib.plugin.c.api.model.ConditionDescriptor</class>
    	<class>org.jqassistant.contrib.plugin.c.api.model.SingleConditionDescriptor</class>
    	<class>org.jqassistant.contrib.plugin.c.api.model.NegationDescriptor</class>
    	<class>org.jqassistant.contrib.plugin.c.api.model.AndDescriptor</class>
    	<class>org.jqassistant.contrib.plugin.c.api.model.OrDescriptor</class>
    </model>
    <scanner>
        <class>org.jqassistant.contrib.plugin.c.impl.scanner.CAstFileScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>

